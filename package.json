    "sass": "^1.32.11",
    "swup": "^2.0.14"
  },
  "browserslist": [
    "last 2 version"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GenzoWeb/newPortfolio.git"
  },
  "bugs": {
