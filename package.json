{
  "name": "portfolio",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "sass": "sass --watch ./src/sass/style.scss:./assets/css/style.css",
    "browser": "browser-sync start --proxy \"localhost/portfolio\" --files \"*.php\" \"src/sass/*.scss\" \"assets/view/*.php\" \"assets/js/*.js\"",
    "github": "git push https://github.com/GenzoWeb/newPortfolio.git",
    "babel": "./node_modules/.bin/babel assets --out-dir assets/lib",
    "prefix": "postcss ./assets/css/style.css -u autoprefixer > ./assets/css/main.css"
  },
  "author": "",
